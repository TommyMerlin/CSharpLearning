<Page x:Class="Drawing.Pages.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">
    <Page.Resources>
        <CombinedGeometry x:Key="ComGeo" GeometryCombineMode="Xor">
            <CombinedGeometry.Geometry1>
                <RectangleGeometry Rect="0 0 200 100"/>
            </CombinedGeometry.Geometry1>
            <CombinedGeometry.Geometry2>
                <RectangleGeometry Rect="0 0 100 200"/>
            </CombinedGeometry.Geometry2>
        </CombinedGeometry>
    </Page.Resources>
    <Grid>
        <Canvas>
            <Path Canvas.Left="5" Canvas.Top="50" Data="{StaticResource ComGeo}"/>
            <Path Canvas.Left="200" Canvas.Top="200" Data="{StaticResource ForbidShape}"/>
        </Canvas>
    </Grid>
</Page>
